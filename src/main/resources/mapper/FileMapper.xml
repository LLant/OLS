<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="org.gdpu.ols.mapper.FileMapper">
    <cache eviction="LRU" flushInterval="60000" readOnly="true"/>
    <resultMap id="student" type="org.gdpu.ols.model.File">
        <id column="id" property="fileId"></id>
        <result column="fileName" property="fileName"/>
        <result column="storageLocation" property="storageLocation"/>
        <result column="publishDate" property="publishDate"/>
        <result column="fileType" property="fileType"/>
        <result column="author" property="author"/>
        <result column="fileTheme" property="fileTheme"/>
    </resultMap>

</mapper>
